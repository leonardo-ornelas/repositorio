				>
				["at0057"] = <
					text = <"*Conclusion(en)Conclusão">
					description = <"*Concise and clinically contextualised narrative interpretation of the pathology test results.(en) Interpretação de uma narrativa concisa e clinicamente contextualizada dos resultados do teste de patologia. ">
